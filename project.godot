config_version=5

[application]

config/name="Immersive System"
run/main_scene="res://scenes/sandbox.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

InputManager="*res://modules/input/core/input_manager.tscn"
FullscreenOnInput="*res://modules/input/extras/fullscreen_on_input.gd"
QuitOnInput="*res://modules/input/extras/quit_on_input.gd"
Console="*res://modules/console/core/console.tscn"
Global="*res://modules/common/global.gd"
InteractionModule="*res://modules/interaction/core/interaction_module.gd"
InventoryModule="*res://modules/inventory/inventory_module.tscn"
InventoryAtlas="*res://modules/inventory/extras/atlas/inventory_atlas.tscn"
PlayerModule="*res://modules/player/interactions/player_module.gd"
WeaponAtlas="*res://modules/weapon/core/weapon_atlas.tscn"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[debug_draw_3d]

settings/3d/volumetric_defaults/thickness=0.01

[dotnet]

project/assembly_name="InteractionSystem"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

[file_customization]

folder_colors={
"res://Resources/": "purple",
"res://assets/": "teal",
"res://modules/": "orange",
"res://resources/": "purple",
"res://scenes/": "blue"
}

