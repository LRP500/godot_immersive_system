config_version=5

[application]

config/name="Immersive System"
run/main_scene="res://scenes/sandbox.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

InteractionModule="*res://modules/interaction/scripts/interaction_module.gd"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[dotnet]

project/assembly_name="InteractionSystem"

[editor_plugins]

enabled=PackedStringArray("res://addons/GoldGdt/plugin.cfg")

[file_customization]

folder_colors={
"res://Resources/": "purple",
"res://assets/": "teal",
"res://modules/": "orange",
"res://resources/": "purple",
"res://scenes/": "blue"
}

