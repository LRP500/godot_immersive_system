; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Immersive System"
run/main_scene="res://scenes/sandbox.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

QuitOnInput="*res://modules/common/input/quit_on_input.gd"
FullscreenOnInput="*res://modules/common/input/fullscreen_on_input.gd"
InputActionMapManager="*res://modules/common/input/action_map/input_action_map_manager.tscn"
InteractionModule="*res://modules/interaction/core/interaction_module.gd"
InventoryModule="*res://modules/inventory/inventory_module.tscn"
InventoryAtlas="*res://modules/inventory/extras/atlas/inventory_atlas.tscn"
PlayerModule="*res://modules/player/interactions/player_module.gd"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[dotnet]

project/assembly_name="InteractionSystem"

[editor_plugins]

enabled=PackedStringArray("res://addons/GoldGdt/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg")

[file_customization]

folder_colors={
"res://Resources/": "purple",
"res://assets/": "teal",
"res://modules/": "orange",
"res://resources/": "purple",
"res://scenes/": "blue"
}

